    "import numpy as np\n",
    "from LogHandler import logger\n",
    "\n",
    "try:\n",
    "\n",
    "    arr = np.arange(1,19)\n",
    "    print(\"Before reversing:\\n\", arr)\n",
    "\n",
    "    print(\"After reversing:\\n\",arr[::-1])\n",
    "    logger.info(\"Array reversed\")\n",
    "\n",
    "except Exception as e:\n",
    "    logger.error(e)"
